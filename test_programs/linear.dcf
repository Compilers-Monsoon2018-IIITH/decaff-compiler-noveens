class Program {
	int arr[ 100000 ];

	void main() {
		int n;
		int to_search;
		boolean fl;
		fl = false;
		n = 5;

		// Seeding rand with current time
		callout("srand", callout("time", 0));

		for i=0, i<n {
			arr[i] = callout("rand") % 10;
		}

		to_search = callout("rand") % 10;

		for i=0,i<n {
			if (arr[i] == to_search) {
				fl = true;
			}
		}

		callout("printf", "arr: ");
		for i=0,i<n {
			callout("printf", "%d ", arr[i]);
		}
		callout("printf", "; to search: %d; ", to_search);

		if (fl == true) {
			callout("printf", "%d exists in the array", to_search);
		}
		else {
			callout("printf", "%d doesnt exist in the array", to_search);
		}

		return;
	}
}