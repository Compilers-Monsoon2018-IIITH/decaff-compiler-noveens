int main(void) {
	int n, i; ReadInteger(n);
	int[] arr;
	arr = NewArray(n, int);

	for (i=0;i<n;i++) {
		int temp; ReadInteger(temp);
		arr[i] = temp;
	}

	int to_search; ReadInteger(to_search);

	int fl = 0;
	for (i=0;i<n;i++) {
		if (arr[i] == to_search) {
			fl = 1;
			break;
		}
	}

	if (fl == 1) {
		Print(to_search, " exists in the array");
	}
	else {
		Print(to_search, " doesn't exist in the array");
	}

	return(0);
}