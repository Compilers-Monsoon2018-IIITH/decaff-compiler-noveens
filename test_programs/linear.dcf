class Program {
	int [ 100000 ] arr;

	int main() {
		int n, i; ReadInteger(n);

		for i=0,i<n {
			int temp; ReadInteger(temp);
			arr[i] = temp;
		}

		int to_search; ReadInteger(to_search);

		int fl = 0;
		for i=0,i<n {
			if (arr[i] == to_search) {
				fl = 1;
				break;
			}
		}

		if (fl == 1) {
			Print(to_search, " exists in the array", "\n");
		}
		else {
			Print(to_search, " doesn't exist in the array", "\n");
		}

		return 0;
	}
}