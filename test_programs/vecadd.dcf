class Program {
	int [ 100000 ] v1;
	int [ 100000 ] v2;

	int main() {
		int n, i; ReadInteger(n);
		
		for i=0,i<n {
			int temp; ReadInteger(temp);
			v1[i] = temp;
		}

		for i=0,i<n {
			int temp; ReadInteger(temp);
			v2[i] = temp;
		}

		for i=0,i<n {
			Print(v1[i] + v2[i], " ");
		}
		Print("\n");

		return 0;
	}
}