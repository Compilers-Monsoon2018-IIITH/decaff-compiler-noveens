class Program {
	int v1[ 100000 ];
	int v2[ 100000 ];

	void main() {
		int n;
		n = 5;

		// Seeding rand with current time
		callout("srand", callout("time", 0));
		
		for i=0,i<n {
			v1[i] = callout("rand") % 100;
		}

		for i=0,i<n {
			v2[i] = callout("rand") % 100;
		}

		callout("printf", "V1: ");
		for i=0,i<n {
			callout("printf", "%d ", v1[i]);
		}

		callout("printf", "; V2: ");
		for i=0,i<n {
			callout("printf", "%d ", v2[i]);
		}

		callout("printf", "; sum: ");
		for i=0,i<n {
			callout("printf", "%d ", v1[i] + v2[i]);
		}

		return;
	}
}