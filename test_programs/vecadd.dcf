class Program {
	int v1[ 100000 ];
	int v2[ 100000 ];

	void main() {
		int n, i; ReadInteger(n);
		
		for i=0,i<n {
			int temp; ReadInteger(temp);
			v1[i] = temp;
		}

		for i=0,i<n {
			int temp; ReadInteger(temp);
			v2[i] = temp;
		}

		for i=0,i<n {
			Print(v1[i] + v2[i], " ");
		}
		Print("\n");

		return;
	}
}