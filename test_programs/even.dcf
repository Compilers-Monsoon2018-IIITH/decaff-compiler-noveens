class Program {
	int arr[ 100000 ];

	void main() {
		int n;
		n = 5;
		
		// Seeding rand with current time
		callout("srand", callout("time", 0));

		for i=0,i<n {
			arr[i] = callout("rand") % 100;
		}

		callout("printf", "arr: ");
		for i=0,i<n {
			callout("printf", "%d ", arr[i]);
		}

		callout("printf", "; even numbers: ");
		for i=0,i<n {
			if (arr[i] % 2 == 0) {
				callout("printf", "%d ", arr[i]);
			}
		}

		return;
	}
}