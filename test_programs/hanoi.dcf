class Program {
	int solve(int n, int s, int t, int a) {
		if (n == 1) {
			Print("disk 1: ", s, " -> ", t, "\n");
		}
		else {
			solve(n-1, s, a, t);
			Print("disk ", n, ": ", s, " -> ", t, "\n");
			solve(n-1, a, t, s);
		}
		return 0;
	}

	int main() {
		int n; ReadInteger(n);
		solve(n, 1, 3, 2);
		return 0;
	}
}